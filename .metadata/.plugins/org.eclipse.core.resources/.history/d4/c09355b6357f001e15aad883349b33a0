package pjav;

public class PrimeNumber {

	
	public static void main(String[] args) {
		
		int num = 15;
		
		for (int i = 2; i < num; i++) {
			if (num%i==0) {
				System.out.println(num);
				System.out.println(i);
				break;
			}
			else {
				System.out.println(num + " is a Prime number");
				}
		}
		
	}
	
	
}
