package pjav;

public class StaticKeyword {

	
	String name;
	String address;
	String city;
	
	StaticKeyword(String name, String address, String city){
		this.name = name;
		this.address = address;
		this.city = city;
	}
	
	public void getDetails() {
		System.out.println(name);
		System.out.println(address);
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		StaticKeyword sk = new StaticKeyword("Ron", "Sky Road", "Manchester");
		StaticKeyword sk1 = new StaticKeyword("James", "MainLand 72", "Manchester");
		sk.getDetails();
		sk1.getDetails();
	}

}
