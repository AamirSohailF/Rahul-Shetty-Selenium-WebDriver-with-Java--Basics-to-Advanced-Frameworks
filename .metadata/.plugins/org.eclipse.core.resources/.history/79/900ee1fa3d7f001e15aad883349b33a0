package pjav;


public class PallindromeOrNot {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		String s = "Kamas";
		String reverse = "";
		
		for (int i = s.length(); i > 0; i++) {
			reverse = reverse + s.charAt(i);
		}
		
		System.out.println(reverse);
		
	}

}
